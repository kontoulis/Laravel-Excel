{
  "name": "maatwebsite/excel",
  "description": "Supercharged Excel exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:kontoulis/PHPExcel.git"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "phpoffice/phpexcel": "dev-laravel-7",
    "illuminate/cache": "^8.0",
    "illuminate/config": "^8.0",
    "illuminate/filesystem": "^8.0",
    "illuminate/support": "^8.0",
    "jeremeamia/superclosure": "*",
    "nesbot/carbon": "*",
    "tijsverkoyen/css-to-inline-styles": "*",
    "laravel/helpers": "*"
  },
  "require-dev": {
    "phpseclib/phpseclib": "*",
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "*",
    "orchestra/testbench": "*",
    "rector/rector": "*"
  },
  "suggest": {
    "illuminate/http": "^8.0",
    "illuminate/routing": "^8.0",
    "illuminate/view": "^8.0",
    "illuminate/queue": "^8.0"
  },
  "autoload": {
    "classmap": [
      "src/Maatwebsite/Excel"
    ],
    "psr-0": {
      "Maatwebsite\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
        "laravel": {
            "providers": [
                "Maatwebsite\\Excel\\ExcelServiceProvider"
            ],
            "aliases": {
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        }
    }
}
